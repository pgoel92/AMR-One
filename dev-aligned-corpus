# ::alignments 1-2|1.1 4-5|1.1.1.1.1 5-6|1.1.1.1.2 6-7|1.1.1.1.3 18-19|1.1.1.1.3 7-8|1.1.1.1.4 8-9|1.1.1.1.5 15-16|1.2 13-14|1.2.1 10-11|1.2.1.1.1.1 12-13|1.2.1.2.1.1 14-15|1.2.1.3.1.1 17-18|1.2.2 19-20|1.2.2.1 0-1|1.2.2.1.1 22-23|1.2.2.1.2 23-24|1.2.2.1.2.1 21-22|1.2.2.1.2.2 16-17|1.2.2.2 * * * * *
(s / say-01
	:ARG0 (r~1 / report-01
		:ARG0 (o / organization
			:name (n / name
				:op1 ("International"~4)
				:op2 ("Campaign"~5)
				:op3 ("to"~6~18)
				:op4 ("Ban"~7)
				:op5 ("Landmines"~8))))
	:ARG1 (m~15 / miss-02
		:ARG0 (a~13 / and
			:op1 (c / country
				:name (n3 / name
					:op1 ("Greece"~10)))
			:op2 (c2 / country
				:name (n4 / name
					:op1 ("Turkey"~12)))
			:op3 (c3 / country
				:name (n5 / name
					:op1 ("Belarus"~14))))
		:ARG1 (d~17 / deadline
			:time-of (d2~19 / destroy-01
				:ARG0 (a2~0 / a)
				:ARG1 (m3~22 / mine
					:ARG1-of (s2~23 / stockpile-01
						:ARG0 (a~13 / and))
					:mod (l~21 / land)))
			:mod (t~16 / treaty)))

# ::id PROXY_APW_ENG_20081121_1082.5
