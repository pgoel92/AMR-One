(c6~6 / clear-04
	:polarity (-~4)
	:ARG1 (i / intend-01
		:ARG0 (p / person
			:name (n / name
				:op1 ("Lavrov"~0))))
	:time (i2~5 / immediate)
	:ARG1-of (c7 / cause-01
		:ARG0 (a~17 / and
			:op1 (b4~14~20 / border-01
				:polarity (-)
				:ARG1 (a2 / and
					:op1 (c / country
						:name (n2 / name
							:op1 ("Russia"~9)))
					:op2 (c2 / country
						:name (n3 / name
							:op1 ("India"~11))))
				:ARG2 (c5 / country
					:name (n6 / name
						:op1 ("Afghanistan"~16~22))))
			:op2 (b3 / border-01
				:ARG1 (c4 / country
					:name (n5 / name
						:op1 ("China"~18)))
				:ARG2 (c5 / country)
				:mod (d / distance-quantity
					:quant (76~25)
					:unit (k~26 / kilometer)
					:mod (o~24 / only)))))

