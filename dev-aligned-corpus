# ::alignments 2-3|1.1.1.1 3-4|1.1.1.2 23-24|1.2 8-9|1.2.1 7-8|1.2.1.1 12-13|1.2.1.2.1 13-14|1.2.1.2.2.1.1.1 14-15|1.2.1.2.2.1.1.2 17-18|1.2.1.2.3 20-21|1.2.1.2.3.1 19-20|1.2.1.2.3.1.1.1 18-19|1.2.1.2.3.1.1.1.1 19-20|1.2.1.2.3.1.1 22-23|1.2.1.2.3.1.2 26-27|1.2.2 29-30|1.2.2.1 30-31|1.2.2.1.1 35-36|1.2.2.1.1.1 44-45|1.2.2.1.1.1.1.1 41-42|1.2.2.1.1.1.1.1.1.1.1 43-44|1.2.2.1.1.1.1.1.2 38-39|1.2.2.1.1.1.1.2 33-34|1.2.2.1.1.1.2 24-25|1.2.2.2.1.1 * * * * *
(s / say-01
	:ARG0 (g / government-organization
		:name (n4 / name
			:op1 ("Foreign"~2)
			:op2 ("Ministry"~3))
		:mod (c / country
			:name (n / name
				:op1 ("Netherlands"))))
	:ARG1 (c4~23 / conclude-01
		:ARG0 (t2~8 / team
			:ARG0-of (i~7 / investigate-01)
			:mod (c / country)
			:ARG1-of (s2 / send-01
				:ARG2 (s3~12 / spot)
				:time (l / late
					:degree (m / more
						:op1 (t / temporal-quantity
							:quant (2~13)
							:unit (w~14 / week))))
				:purpose (g2~17 / gather-01
					:ARG0 (t2~8 / team)
					:ARG1 (a~20 / and
						:op1 (t3~19 / thing
							:ARG0-of (e~19 / evidence-01
								:mod (f~18 / forensic)))
						:op1 (a2~22 / account-01
							:ARG0 (p / person
								:ARG0-of (w2 / witness-01
									:mod (e2 / eye))))))))
		:ARG1 (k~26 / kill-01
			:ARG0 (m2~29 / munition
				:ARG1-of (p3~30 / propel-01
					:ARG0 (r~35 / rocket
						:ARG1-of (f2 / find-01
							:location (a3~44 / arsenal
								:poss (c3 / country
									:name (n3 / name
										:op1 ("Russia"~41)))
								:mod (m3~43 / military))
							:mod (o~38 / only))
						:mod (t4~33 / type))))
			:ARG1 (p2 / person
				:name (n5 / name
					:op1 ("Storimans"~24)))))

