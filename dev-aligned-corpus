			:ARG0 (a~19 / abuse-01
				:ARG1 (p / person)
				:ARG1 (h~24 / heroin
					:source (c3 / country
						:name (n3 / name
							:op1 ("Afghanistan"))))
				:instrument (n4~27 / needle
					:ARG1-of (i2~26 / infect-01))))
		:ARG1-of (c4 / cause-01
			:ARG0 (i~14 / illness
				:ARG1-of (r~13 / relate-01
