	:ARG1 (p / person
		:name (n2 / name
			:op1 ("Dmitry"~3)
			:op2 ("Medvedev"~4))
		:ARG0-of (h / have-org-role-91
			:ARG1 (c / country
				:name (n / name
					:op1 ("Russian"~0~19)
					:op2 ("Federation"~1~20)))
			:ARG2 (p2~2 / president)))
	:time (d / date-entity
		:month (5~9)
		:day (15~10)
		:year (2008~12))
	:manner (a2~29 / and
		:op1 (t~25 / tour-01
			:ARG1 (p / person)
			:ARG1 (b~28 / base
				:mod (m~27 / missile)))
		:op2 (p4~30 / promise-01
			:ARG1 (p / person)
			:ARG1 (p5~32 / provide-01
				:ARG1 (p / person)
				:ARG1 (f2~35 / fund-01
					:ARG1-of (n3 / need-01
						:purpose (c4~39 / counter-01
							:ARG0 (f3~23~38 / force
								:mod (n4 / nucleus))
							:ARG1 (t2 / thing
								:ARG1-of (t3 / threaten-01
									:ARG2 (g / globe)))))))))
	:prep-as (p3 / person
		:ARG0-of (h3 / have-org-role-91
			:ARG1 (f / force
				:ARG1-of (a~22 / arm-01)
				:ARG1 (c / country))
			:ARG2 (c5 / commander-in-chief)))

