(t / tell-01
	:ARG0 (p / person
		:name (n / name
			:op1 ("Bernard"~8)
			:op2 ("Kouchner"~9))
		:ARG0-of (h / have-org-role-91
			:ARG1 (a~17 / and
				:op1 (c / country
					:name (n3 / name
						:op1 ("Austria")))
				:op2 (c2 / country
					:name (n4 / name
						:op1 ("France"))))
			:ARG2 (m~7 / minister
				:mod (f~6 / foreign))))
	:ARG2 (p2 / person
		:ARG0-of (h2 / have-org-role-91
			:ARG1 (o~14 / organization
				:name (n2 / name
					:op1 ("Organization")
					:op2 ("for"~15)
					:op3 ("Security"~16)
					:op4 ("and")
					:op5 ("Cooperation"~18)
					:op6 ("in"~19)
					:op7 ("Europe"~20)))
			:ARG2 (m2~11 / member)))
	:time (d / date-entity
		:day (17~1)
		:month (7~2)
		:year (2008~3))

