# ::tok 0-The 1-3 2-Swedes 3-were 4-arrested 5-in 6-raids 7-in 8-Stockholm 9-in 10-February 11-.
# ::alignments 4-5|1 1-2|1.1.1 6-7|1.2 8-9|1.3.1.1 10-11|1.4.1 * * * * *
(a~4 / arrest-01
	:ARG1 (p2 / person
		:quant (3~1)
		:mod (c / country
			:name (n / name
				:op1 ("Sweden"))))
	:time (r~6 / raid-01)
	:location (c2 / city
		:name (n2 / name
			:op1 ("Stockholm"~8)))
	:time (d / date-entity
		:month (2~10))

