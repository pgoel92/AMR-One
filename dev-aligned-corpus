	:ARG0 (p2~0 / person
		:ARG1-of (e~0 / expert-41))
	:ARG1 (d~8 / doctor-01
		:ARG1 (p~6 / photo
			:mod (l~5~14 / launch-01
				:ARG1 (m~4~13 / missile)
				:location (c / country
					:name (n / name
						:op1 ("Iran")))))
		:purpose (s2~10 / show-01
			:ARG1 (l2 / launch-01
