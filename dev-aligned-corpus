	:ARG0 (r~2 / report-01
		:ARG0 (o / organization
			:name (n / name
				:op1 ("International"~5~24)
				:op2 ("Campaign"~6)
				:op3 ("to"~7~26)
				:op4 ("Ban"~8)
				:op5 ("Landmine"~9)))
		:time (d3 / date-entity
			:month (11)
			:day (21)
			:year (2008)))
	:ARG1 (m~17 / miss-02
		:ARG0 (a~14 / and
			:op1 (c / country
				:name (n3 / name
					:op1 ("Greece"~11)))
			:op2 (c2 / country
				:name (n4 / name
					:op1 ("Turkey"~13)))
			:op3 (c3 / country
				:name (n5 / name
					:op1 ("Belarus"~15))))
