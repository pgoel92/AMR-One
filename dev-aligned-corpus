# ::snt International; narcotics; crime; Government; terrorism; health
# ::save-date
# ::tok 0-International 1-; 2-narcotics 3-; 4-crime 5-; 6-Government 7-; 8-terrorism 9-; 10-health
# ::alignments 0-1|1.1 2-3|1.2 4-5|1.3 6-7|1.4.1 8-9|1.5 10-11|1.6 * * * * *
(a / and
	:op1 (i~0 / international)
	:op2 (n~2 / narcotic)
	:op3 (c~4 / crime)
	:op4 (g / government-organization
		:ARG0-of (g2~6 / govern-01))
	:op5 (t~8 / terrorism)
	:op6 (h~10 / health)

# ::id PROXY_APW_ENG_20080515_0628.4
# ::snt Russia proposes cooperation with India and China to increase security around Afghanistan to block drug supplies.
# ::save-date
# ::tok 0-Russia 1-proposes 2-cooperation 3-with 4-India 5-and 6-China 7-to 8-increase 9-security 10-around 11-Afghanistan 12-to 13-block 14-drug 15-supplies 16-.
# ::alignments 1-2|1 0-1|1.1.1.1 2-3|1.2 5-6|1.2.1 4-5|1.2.1.1.1.1 6-7|1.2.1.2.1.1 8-9|1.3 9-10|1.3.1 10-11|1.3.2 11-12|1.3.2.1.1.1 13-14|1.3.3 15-16|1.3.3.1 14-15|1.3.3.1.1 * * * * *
(p~1 / propose-01
	:ARG0 (c / country
		:name (n / name
			:op1 ("Russia"~0)))
	:ARG1 (c5~2 / cooperate-01
		:ARG0 (c / country)
		:ARG1 (a~5 / and
			:op1 (c2 / country
				:name (n2 / name
					:op1 ("India"~4)))
			:op2 (c3 / country
				:name (n3 / name
					:op1 ("China"~6)))))
	:purpose (i~8 / increase-01
		:ARG1 (s~9 / security)
		:location (a2~10 / around
			:op1 (c4 / country
				:name (n4 / name
					:op1 ("Afghanistan"~11))))
		:purpose (b~13 / block-01
			:ARG1 (s2~15 / supply-01
				:ARG1 (d~14 / drug))))

# ::id PROXY_APW_ENG_20080515_0628.5
# ::snt Afghanistan is 1 of the world's major opium producers and supplies Western markets through transit countries such as Russia.
# ::save-date
# ::tok 0-Afghanistan 1-is 2-1 3-of 4-the 5-world 6-'s 7-major 8-opium 9-producers 10-and 11-supplies 12-Western 13-markets 14-through 15-transit 16-countries 17-such 18-as 19-Russia 20-.
# ::alignments 10-11|1 9-10|1.1 16-17|1.1.1 0-1|1.1.1.1.1 5-6|1.1.1.2.1 8-9|1.1.2 7-8|1.1.3 11-12|1.2 13-14|1.2.1 15-16|1.2.2.1 19-20|1.2.2.2.1.1 * * * * *
(a~10 / and
	:op1 (p~9 / produce-01
		:ARG0 (c~16 / country
			:name (n / name
				:op1 ("Afghanistan"~0))
			:ARG1-of (i / include-91
				:ARG2 (w~5 / world)))
		:ARG1 (o~8 / opium)
		:mod (m~7 / major))
	:op2 (s~11 / supply-01
		:ARG0 (c~16 / country)
		:ARG2 (m2~13 / market
			:location (w2 / world-region
				:name (n3 / name
					:op1 ("West"))))
		:path (c3 / country
			:ARG2-of (t~15 / transit-01)
			:example-of (c2 / country
				:name (n2 / name
					:op1 ("Russia"~19)))))

# ::id PROXY_APW_ENG_20080515_0628.6
# ::snt On 15 May 2008 the Government of Russia proposed cooperation with India and China to increase security around Afghanistan in order to block drug supplies.
# ::save-date
# ::tok 0-On 1-15 2-May 3-2008 4-the 5-Government 6-of 7-Russia 8-proposed 9-cooperation 10-with 11-India 12-and 13-China 14-to 15-increase 16-security 17-around 18-Afghanistan 19-in 20-order 21-to 22-block 23-drug 24-supplies 25-.
# ::alignments 8-9|1 5-6|1.1.1 7-8|1.1.1.1.1.1 9-10|1.2 12-13|1.2.1 11-12|1.2.1.1.1.1 13-14|1.2.1.2.1.1 2-3|1.3.1 1-2|1.3.2 3-4|1.3.3 15-16|1.4 16-17|1.4.1 17-18|1.4.2 18-19|1.4.2.1.1.1 22-23|1.4.3 24-25|1.4.3.1 23-24|1.4.3.1.1 * * * * *
(p~8 / propose-01
	:ARG0 (g / government-organization
		:ARG0-of (g2~5 / govern-01
			:ARG1 (c / country
				:name (n / name
					:op1 ("Russia"~7)))))
	:ARG1 (c5~9 / cooperate-01
		:ARG0 (g / government-organization)
		:ARG1 (a~12 / and
			:op1 (c2 / country
				:name (n2 / name
					:op1 ("India"~11)))
			:op2 (c3 / country
				:name (n3 / name
					:op1 ("China"~13)))))
	:time (d / date-entity
		:month (5~2)
		:day (15~1)
		:year (2008~3))
	:purpose (i~15 / increase-01
		:ARG1 (s~16 / security)
		:location (a2~17 / around
			:op1 (c4 / country
				:name (n4 / name
					:op1 ("Afghanistan"~18))))
		:purpose (b~22 / block-01
			:ARG1 (s2~24 / supply-01
				:ARG1 (d2~23 / drug))))

# ::id PROXY_APW_ENG_20080515_0628.7
# ::snt Russian Foreign Minister Sergey Lavrov at a joint news conference in Yekaterinburg with the foreign ministers of India and China stated that tightening security at Afghan borders would decrease the threat of drugs and terrorism.
# ::save-date
# ::tok 0-Russian 1-Foreign 2-Minister 3-Sergey 4-Lavrov 5-at 6-a 7-joint 8-news 9-conference 10-in 11-Yekaterinburg 12-with 13-the 14-foreign 15-ministers 16-of 17-India 18-and 19-China 20-stated 21-that 22-tightening 23-security 24-at 25-Afghan 26-borders 27-would 28-decrease 29-the 30-threat 31-of 32-drugs 33-and 34-terrorism 35-.
# ::alignments 20-21|1 3-4|1.1.1.1 4-5|1.1.1.2 2-3|1.1.2.2 15-16|1.1.2.2 1-2|1.1.2.2.1 14-15|1.1.2.2.1 28-29|1.2 22-23|1.2.1 23-24|1.2.1.1 26-27|1.2.1.1.1 18-19|1.2.2.1 33-34|1.2.2.1 32-33|1.2.2.1.1 34-35|1.2.2.1.2 9-10|1.3 8-9|1.3.1 7-8|1.3.2 11-12|1.3.3.1.1 17-18|1.3.4.1.1.1.1.1 19-20|1.3.4.2.1.1.1.1 * * * * *
(s~20 / state-01
	:ARG0 (p / person
		:name (n2 / name
			:op1 ("Sergey"~3)
			:op2 ("Lavrov"~4))
		:ARG0-of (h / have-org-role-91
			:ARG1 (c / country
				:name (n / name
					:op1 ("Russia")))
			:ARG2 (m~2~15 / minister
				:mod (f~1~14 / foreign))))
	:ARG1 (d~28 / decrease-01
		:ARG0 (t~22 / tighten-01
			:ARG1 (s2~23 / security
				:location (b~26 / border
					:mod (c6 / country
						:name (n6 / name
							:op1 ("Afghanistan"))))))
		:ARG1 (t2 / threaten-01
			:ARG1 (a2~18~33 / and
				:op1 (d2~32 / drug)
				:op2 (t3~34 / terrorism))))
	:time (c4~9 / conference
		:mod (n7~8 / news)
		:mod (j~7 / joint)
		:location (c5 / city
			:name (n3 / name
				:op1 ("Yekaterinburg"~11)))
		:accompanier (a / and
			:op1 (p2 / person
				:ARG0-of (h2 / have-org-role-91
					:ARG1 (c2 / country
						:name (n4 / name
							:op1 ("India"~17)))
					:ARG2 (m2 / minister
						:mod (f2 / foreign))))
			:op2 (p3 / person
				:ARG0-of (h3 / have-org-role-91
					:ARG1 (c3 / country
						:name (n5 / name
							:op1 ("China"~19)))
					:ARG2 (m3 / minister
						:mod (f3 / foreign))))))

