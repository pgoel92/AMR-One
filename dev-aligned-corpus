	:domain (t2 / threaten-01
		:ARG0 (c6 / capable-41
			:ARG1 (c / country
				:name (n / name
					:op1 ("China"~0)))
			:ARG2 (s~4 / shoot-05
				:ARG1 (s2~6~25 / satellite)))
		:ARG1-of (a2 / add-02))
	:time (e~14 / event
		:mod (h~16 / hostility
			:mod (o~17 / over
				:mod (c2 / country
					:name (n2 / name
						:op1 ("Taiwan"~18))))))
	:ARG1-of (c4 / cause-01
		:ARG0 (r~23 / rely-01
			:ARG0 (m~22 / military
				:mod (c3 / country
					:name (n3 / name
						:op1 ("U.S."~21))))
			:ARG1 (s3 / satellite)
			:ARG2 (a3~34 / and
				:op1 (r2~27 / reconnaissance)
				:op2 (n4~29 / navigate-01)
				:op3 (s4~33 / system
					:ARG0-of (g / guide-01
