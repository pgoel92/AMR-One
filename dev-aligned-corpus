(s~4 / state-01
	:ARG0 (g / government-organization
		:ARG0-of (g2~1 / govern-01
			:ARG1 (c / country
				:name (n / name
					:op1 ("Russia"~3)))))
	:ARG1 (d~9 / die-01
		:ARG1 (p / person
			:ARG1 (c / country)
			:quant (m / multiple
				:op1 (1000)))
