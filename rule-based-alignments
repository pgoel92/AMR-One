# ::save-date
# ::tok 0-Alan 1-Spoon 2-, 3-recently 4-named 5-Newsweek 6-president 7-, 8-said 9-Newsweek 10-'s 11-ad 12-rates 13-would 14-increase 15-5 16-% 17-in 18-January 19-.
# ::alignments 8-9|1 4-5|1.1.1 0-1|1.1.1.1 1-2|1.1.1.2 6-7|1.1.2.1 5-6|1.1.2.1.1.1.1.1 3-4|1.1.2.2 14-15|1.2 12-13|1.2.1 16-17|1.2.2 15-16|1.2.2.1 18-19|1.2.3.1 * * * * *
(s / say-01
      :ARG0 (p / person
            :name (a / name :op1 "Alan" :op2 "Spoon")
            :ARG1-of (n2 / name-03
                  :ARG2 (p3 / president
                        :ARG2-of (h / have-org-role-91
                              :ARG0 p
                              :ARG1 (c / company
                                    :name (n / name :op1 "Newsweek"))))
                  :time (r / recent)))
      :ARG1 (i / increase-01
            :ARG0 c
            :ARG1 (r2 / rate
                  :mod (a2 / advertise-01)
                  :poss c)
            :ARG2 (p2 / percentage-entity :value 5)
            :time (d / date-entity :month 1)))

# ::id nw.wsj_0012.5
# ::snt A full , four - color page in Newsweek will cost $ 100,980 .
# ::save-date
# ::tok 0-A 1-full 2-, 3-four 4-- 5-color 6-page 7-in 8-Newsweek 9-will 10-cost 11-$ 12-100,980 13-.
# ::alignments 10-11|1 6-7|1.1 5-6|1.1.1 1-2|1.1.2 8-9|1.1.3.1.1 11-12|1.2.2 * * * * *
(c / cost-01
      :ARG1 (p / page
            :mod (c2 / color :quant 4)
            :mod (f / full)
            :part-of (m2 / magazine
                  :name (n / name :op1 "Newsweek")))
      :ARG2 (m / monetary-quantity :quant 100980
            :unit (d / dollar)))

