            :condition (w / wish-01
                  :ARG0 p
                  :ARG1 l))
      :ARG2 (p / person
            :name (n / name :op1 "Anonymous" :op2 "6:37")))

# ::id wb.eng_0003.97
# ::snt But you do n't have a right to have the rest of the state to underwrite construction of a $ 400 million highway to make it easier to shop and get to work .
# ::save-date
# ::tok 0-But 1-you 2-do 3-n't 4-have 5-a 6-right 7-to 8-have 9-the 10-rest 11-of 12-the 13-state 14-to 15-underwrite 16-construction 17-of 18-a 19-$ 20-400 21-million 22-highway 23-to 24-make 25-it 26-easier 27-to 28-shop 29-and 30-get 31-to 32-work 33-.
# ::alignments 15-16|1.1.2 10-11|1.1.2.1 13-14|1.1.2.1.1 22-23|1.1.2.2.1 29-30|1.1.2.2.2.1 28-29|1.1.2.2.2.1.1 30-31|1.1.2.2.2.1.2 32-33|1.1.2.2.2.1.2.1 1-2|1.1.3 * * * * *
(c2 / contrast-01
      :ARG2 (e2 / entitle-01 :polarity -
            :ARG1 (u / underwrite-01
                  :ARG0 (r2 / rest
                        :part-of (s / state))
                  :ARG1 (c / construct-01
                        :ARG1 (h / highway
                              :ARG1-of (c3 / cost-01
                                    :ARG2 (m / monetary-quantity :quant 400000000
                                          :unit (d / dollar))))
                        :purpose (e / ease-02
                              :ARG1 (a / and
                                    :op1 (s2 / shop-01)
                                    :op2 (g / get-05
                                          :ARG2 (w / work))))))
            :ARG2 (y / you)))

