            :ARG1-of (s2 / spend-01
                  :ARG3 (m2 / monetary-quantity :quant 400000000
                        :unit (d2 / dollar))
                  :mod (p / possible)))
      :manner (c / consistent)
      :prep-in (d3 / debate-01
            :ARG1 (r / road
                  :name (n / name :op1 288))))

# ::id wb.eng_0003.99
# ::snt How much traffic congestion could have been alleviated had the money been applied to other pressing road projects or , god forbid , even non-road projects .
# ::save-date
# ::tok 0-How 1-much 2-traffic 3-congestion 4-could 5-have 6-been 7-alleviated 8-had 9-the 10-money 11-been 12-applied 13-to 14-other 15-pressing 16-road 17-projects 18-or 19-, 20-god 21-forbid 22-, 23-even 24-non 25-- 26-road 27-projects 28-.
# ::alignments 2-3|1.1.1.1 1-2|1.1.1.2 10-11|1.2.1 18-19|1.2.2 16-17|1.2.2.1.1 26-27|1.2.2.1.1 14-15|1.2.2.1.3 16-17|1.2.2.2.1 26-27|1.2.2.2.1 25-26|1.2.2.2.1.1 23-24|1.2.2.2.2 21-22|1.2.2.2.3 20-21|1.2.2.2.3.2 * * * * *
(p / possible
      :domain (a / alleviate-01
            :ARG1 (c / congest-01
                  :ARG2 (t / traffic)
                  :quant (m2 / much)))
      :condition (a3 / apply-02
            :ARG1 (m / money)
            :ARG2 (o / or
                  :op1 (p2 / project
                        :mod (r / road)
                        :ARG2-of (p3 / press-01)
                        :mod (o2 / other))
                  :op2 (p4 / project
                        :mod (r2 / road :polarity -)
                        :mod (e / even)
                        :ARG1-of (f / forbid-01 :mode imperative
                              :ARG0 (g / god))))))

