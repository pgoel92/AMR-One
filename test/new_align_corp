      :ARG1 (t / thing
            :mod (o2 / other)
            :ARG1-of (s2 / spend-01
                  :ARG3 (m2 / monetary-quantity :quant 400000000
                        :unit (d2 / dollar))
                  :mod (p / possible)))
      :manner (c / consistent)
      :prep-in (d3 / debate-01
            :ARG1 (r / road
                  :name (n / name :op1 288))))

# ::id wb.eng_0003.99
# ::snt How much traffic congestion could have been alleviated had the money been applied to other pressing road projects or , god forbid , even non-road projects .
