# ::alignments 0-1|1 4-5|1.1 1-2|1.1.1 6-7|1.1.2 9-10|1.1.2.1 3-4|1.1.3 * * * * *
(a / and
      :op2 (p / pay-01
            :ARG0 (i / i)
            :ARG3 (l / live-01
                  :ARG0 i
                  :location (c / country))
            :mod (s / still)))

# ::id wb.eng_0003.94
# ::snt Many people have legitimate preferences - horses , hunting , gardening , certain types of small business - that makes them NOT want to live in an urban core .
# ::tok Many people have legitimate preferences - horses , hunting , gardening , certain types of small business - that makes them NOT want to live in an urban core .
# ::alignments 0-1|1.1.1 16-17|1.2.4 15-16|1.2.4.1 12-13|1.2.4.2.1 3-4|1.3 22-23|1.4.1 5-6|1.4.1.1 17-18|1.4.1.1 24-25|1.4.1.2 28-29|1.4.1.2.1 27-28|1.4.1.2.1.1 * * * * *
(p / prefer-01
      :ARG0 (p2 / person
            :quant (m / many))
      :ARG1 (a / and
            :op1 (h / horse)
            :op2 (h2 / hunt-01)
            :op3 (g / garden-01)
            :op4 (b / business
                  :mod (s / small)
                  :mod (t / type
                        :mod (c / certain))))
      :mod (l / legitimate)
      :ARG0-of (m2 / make-02
            :ARG1 (w / want-01 :polarity -
                  :ARG0 p2
                  :ARG1 (l2 / live-01
                        :location (c3 / core
                              :mod (u / urban))))))

